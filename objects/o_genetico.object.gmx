instance_create(820, 200, o_limite);
instance_create(820, 360, o_pobla);

ini_open("patrones.ini");
    
    patrones = ini_read_real("config", "muestras", 1);
    
    px[patrones - 1, m_xin - 1] = 0;
    pd[patrones - 1, 1] = 0;
    for (var p = 0; p &lt; patrones; p++)
    {
        for (var i = 0; i &lt; m_xin; i++)
        {
