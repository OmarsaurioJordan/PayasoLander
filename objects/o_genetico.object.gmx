        }
        pd[p, 0] = ini_read_real("p" + string(p), "d0", -1);
        pd[p, 1] = ini_read_real("p" + string(p), "d1", 0);
    }
    
ini_close();

error = -1;
contador = 0;
iteraciones = 0;

for (var n = 0; n &lt; o_pobla.numerico; n++)
{
    ind[n] = instance_create(0, 0, o_red);
    s_escalar_red(ind[n], 2);
}
