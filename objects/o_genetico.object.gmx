    <event eventtype="3" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>///algoritmo
if entrenando
{
    var aux1, aux2, n, l, r, meetpool, s1, s2, nuevas;
    
    aux1 = ds_priority_create();
    aux2 = ds_list_create();
    for (n = 0; n &lt; o_pobla.numerico; n++)
    {
        s_calcula_error(ind[n], id);
        ds_priority_add(aux1, ind[n], ind[n].error);
        ds_list_add(aux2, ind[n]);
    }
    meetpool[0] = ds_priority_find_min(aux1);
    ds_priority_destroy(aux1);
    ds_list_delete(aux2, ds_list_find_index(aux2, meetpool[0]));
    
    for (l = 1; l &lt; o_seleccionar.numerico; l++)
    {
        s1 = ds_list_find_value(aux2, irandom(o_pobla.numerico - (l + 1)));
        s2 = ds_list_find_value(aux2, irandom(o_pobla.numerico - (l + 1)));
        if s1.error &lt;= s2.error
        {
            meetpool[l] = s1;
        }
        else
        {
            meetpool[l] = s2;
        }
        ds_list_delete(aux2, ds_list_find_index(aux2, meetpool[l]));
    }
    ds_list_destroy(aux2);
    
    nuevas[0] = meetpool[0];
    n = 1;
    if o_pesos.numerico == 1
    {
        for (l = 0; l &lt; o_seleccionar.numerico; l++)
        {
            for (r = 0; r &lt; o_num_mut.numerico; r++)
            {
                if n &lt; o_pobla.numerico
                {
                    nuevas[n] = instance_create(0, 0, o_red);
                    s_mutar_fast(nuevas[n], meetpool[l]);
                    n++;
                }
            }
        }
    }
    else
    {
        for (l = 0; l &lt; o_seleccionar.numerico; l++)
        {
            for (r = 0; r &lt; o_num_mut.numerico; r++)
            {
                if n &lt; o_pobla.numerico
                {
                    nuevas[n] = instance_create(0, 0, o_red);
                    s_mutar(nuevas[n], meetpool[l]);
                    n++;
                }
            }
        }
    }
    while n &lt; o_pobla.numerico
    {
        nuevas[n] = instance_create(0, 0, o_red);
        n++;
    }
    
    for (n = 0; n &lt; o_pobla.numerico; n++)
    {
        with ind[n]
        {
            if id != nuevas[0]
            {
                instance_destroy();
            }
        }
    }
    
    ind = nuevas;
    error = ind[0].error;
    
    contador++;
    iteraciones++;
    
    if contador &gt;= o_limite.numerico or error &lt;= o_deseado.numerico
    {
        entrenando = false;
        audio_play_sound(a_finentreno, 5, false);
    }
}
</string>
          </argument>
        </arguments>
      </action>
    </event>
