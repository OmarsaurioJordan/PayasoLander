            <string>if mouse_check_button_pressed(mb_left)
{
    
    //boton play
    if point_distance(mouse_x, mouse_y, 512, 200) &lt; 48
    {
        s_guarda_parametros_menu();
        o_cuentawins.wins = 0;
        o_cuentawins.casi = 0;
        o_cuentawins.lose = 0;
        room_goto(w_juego);
    }
    
    //boton de gradiente
    else if point_distance(mouse_x, mouse_y, 256, 384) &lt; 48
    {
        s_guarda_parametros_menu();
        room_goto(w_gradiente);
    }
    
    //boton de geneticas
    else if point_distance(mouse_x, mouse_y, 512, 384) &lt; 48
    {
        s_guarda_parametros_menu();
        room_goto(w_genetico);
    }
    
    //boton de particulas
    else if point_distance(mouse_x, mouse_y, 768, 384) &lt; 48
    {
        s_guarda_parametros_menu();
        room_goto(w_particulas);
    }
}

if keyboard_check_pressed(vk_anykey)
{
    switch keyboard_key
    {
        case vk_escape:
            game_end();
            break;
        
        case ord("M"):
            url_open("https://www.dropbox.com/home/jueguchosplanes/Juegosomi");
            break;
        
        case ord('1'):
            m_todolado = !m_todolado;
            break;
        
        case ord('2'):
            m_dmnn = !m_dmnn;
            break;
        
        case ord('3'):
            var archivo = get_save_filename("TXT|*.txt", "patrones");
            if archivo != ""
            {
                //s_guarda_patrones_txt(true, false);
                //s_guarda_patrones_txt(false, false);
                s_guarda_patrones_txt(false, true);
                file_copy("patrones.txt", archivo);
            }
            break;
        
        case ord('4'):
            var archivo = get_open_filename("TXT|*.txt", "");
            if archivo != ""
            {
                file_copy(archivo, "dmnn.txt");
            }
            break;
        
        case ord('5'):
            var archivo = get_open_filename("INI|*.ini", "");
            if archivo != ""
            {
                file_copy(archivo, "R123.ini");
                ini_open("R123.ini");
                m_ocult = real(ini_read_string("datos", "ocultas", "1"));
                ini_close();
                file_copy("R123.ini", "red_oc" + string(m_ocult) + ".ini");
                file_delete("R123.ini");
            }
            break;
        
        case ord('6'):
            var archivo = get_save_filename("INI|*.ini", "mlp" + string(m_ocult));
            if archivo != ""
            {
                if file_exists("red_oc" + string(m_ocult) + ".ini")
                {
                    file_copy("red_oc" + string(m_ocult) + ".ini", archivo);
                }
            }
            break;
        
        case ord('0'):
            screen_save("foto.png");
            var archivo = get_save_filename("PNG|*.png", "imagen");
            if archivo != ""
            {
                file_copy("foto.png", archivo);
            }
            break;
    }
}
</string>
