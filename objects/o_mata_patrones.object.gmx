            <string>depth = 200;

ini_open("patrones.ini");
    
    patrones = string(ini_read_real("config", "muestras", 1));
    datap = "";
    
    if real(patrones) &gt; 1
    {
        var d0, d1;
        d0 = 0;
        d1 = 0;
        tt = 0;
        
        for (var p = 0; p &lt; real(patrones); p++)
        {
            if ini_read_real("p" + string(p), "d0", 0) == 1
            {
                d0++;
            }
            
            if ini_read_real("p" + string(p), "d1", 0) != 0
            {
                d1++;
            }
            else if ini_read_real("p" + string(p), "d0", 0) != 1
            {
                tt++;
            }
        }
        
        datap += "#impulsos: " + string(round(d0 / real(patrones) * 100)) + "%";
        datap += "#giros: " + string(round(d1 / real(patrones) * 100)) + "%";
        datap += "#inactivo: " + string(round(tt / real(patrones) * 100)) + "%";
        
        patrones += " en " + string(ini_read_real("config", "juegos", 0));
    }
    
ini_close();
</string>
          </argument>
